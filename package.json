    "less-loader": "^2.2.3",
    "rimraf": "^2.6.2",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.6",
    "webpack": "^2.1.0-beta.28",
    "webpack-merge": "^1.1.2"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.2.5",
    "bootstrap": "^3.3.7",
    "bootstrap-multiselect": "^0.9.13-1",
    "bootstrap-tagsinput": "^0.7.1",
    "jquery": "^3.2.1",
    "jquery-date-range-picker": "^1.0.4",
    "jquery-ui": "^1.12.1",
    "jquery.nicescroll": "^3.7.6",
    "jquery.uniform": "^4.2.2",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "pickadate": "^3.5.6",
